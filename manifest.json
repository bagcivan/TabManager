{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Manage your tabs in groups",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage",
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}